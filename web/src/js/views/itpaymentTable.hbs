<div class="container">
    <div class="row col-sm-3 pull-right">
        <h4><a class = "btn btn-success" id="js-addItPayment" style="margin-left: -39px;" style="margin-left: -38;"><i class="fa fa-plus"></i>&nbsp Add New </a></h4>
    </div>
    <div class="row db-itPayment" style="width: 1020px">
        <table id="itpaymentexample" class="display example_wrapper" cellspacing="0" width="100%">
            <thead>
                <tr>
                    <th>BusinessDate</th>
                    <th>Employee Initials</th>
                    <th>Checked Status </th>
                    <th>Reported Date</th>
                    <th>Reported Time</th>
                </tr>
            </thead>
            <tbody>
                {{#each tablerows}}
                    <tr>
                        <td>{{bdate}}</td>
                        <td>{{employee_initials}}</td>
                        {{#equaltoo checked_status 1}}
                            <td> Yes </td>
                            {{else}}
                                <td> No </td>
                        {{/equaltoo}}
                        <td>{{date}}</td>
                        <td>{{time}}</td>
                    </tr>
                {{/each}}
            </tbody>
        </table>
    </div>
</div>
